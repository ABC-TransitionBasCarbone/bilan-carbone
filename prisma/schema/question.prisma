model Question {
  id              String  @id @default(uuid())
  idIntern        String  @default(uuid())
  label           String
  order           Int     @default(autoincrement())
  subPost         SubPost
  type            Type
  PossibleAnswers Json?
}

enum Type {
  QCM
  QCU
  SELECT
  TABLE
  CP
  date
  number
  range
  tel
  text
}
