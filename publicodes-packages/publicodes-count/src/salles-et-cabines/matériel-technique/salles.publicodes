salles et cabines . matériel technique . salles:
  #TODO: Should be a list of salles
  titre: Salles
  formule: salle
  par défaut: 0
  unité: kgCO2e

salles et cabines . matériel technique . salle:
  titre: Salle
  formule:
    somme:
      - projecteur
      - écran
      - fauteuils
      - système son
  unité: kgCO2e

salles et cabines . matériel technique . salle . projecteur:
  titre: Projecteur
  formule:
    variations:
      - si: age > amortissement
        alors: 0
      - sinon: FE / amortissement
  par défaut: 0
  unité: kgCO2e
  avec:
    age:
      durée:
        unité: an
        depuis: année achat
        jusqu'à: commun . année actuelle
      arrondi: oui

    amortissement:
      valeur: 10
      note: La durée d'amortissement est la même pour tous les projecteurs et fixée à 10 ans.

    année achat:
      question: Année d'acquisition projecteur
      type: date
      form:
        saisie: année

    FE:
      titre: Facteur d'émission du projecteur
      formule:
        variations:
          - si: type = "xénon"
            alors: type . xénon . FE
          - si: type = "laser"
            alors: type . laser . FE
      unité: kgCO2e

